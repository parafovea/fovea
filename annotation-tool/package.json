{
  "name": "@fovea/annotation-tool",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build && open dist/stats.html",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "playwright test --project=smoke",
    "test:e2e:regression": "playwright test --project=regression",
    "test:e2e:a11y": "playwright test --project=accessibility",
    "test:e2e:a11y:ui": "playwright test --project=accessibility --ui",
    "test:e2e:a11y:debug": "playwright test --project=accessibility --debug",
    "test:e2e:visual": "playwright test --project=visual",
    "test:e2e:visual:update": "playwright test --project=visual --update-snapshots",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "docs": "typedoc --options typedoc.json"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@mui/x-date-pickers": "^8.11.3",
    "@reduxjs/toolkit": "^2.2.1",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/leaflet": "^1.9.20",
    "@types/lodash": "^4.17.20",
    "@types/uuid": "^10.0.0",
    "ajv": "^8.12.0",
    "axios": "^1.6.7",
    "camelcase-keys": "^10.0.1",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-leaflet": "^4.2.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "snakecase-keys": "^9.0.2",
    "uuid": "^13.0.0",
    "video.js": "^8.10.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/video.js": "^7.3.57",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "axe-playwright": "^2.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
